# Tint2 Panel
~/.scripts/tint2-min.sh &

# Touchpad tap
sh ~/.scripts/touchpad.sh &

# Set the background
hsetroot -cover ~/.wallpaper/current-wallpaper.jpg &

# Fonts
sh ~/.scripts/tangerine.sh &

# Compton
compton -r 12 -o .4 -c -C -l -17 -t -7 -z -D 8 -I 0.05 -O 0.07 -f &

# Volume Icon
volumeicon &

# Network Manager
nm-applet &
