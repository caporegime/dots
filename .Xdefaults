! ~~~~~~
! fonts
! ~~~~~~
*font: -aaron-bitbuntufull-medium-r-normal--10-100-72-72-c-90-utf8-1
*boldFont: -aaron-bitbuntufull-medium-r-normal--10-100-72-72-c-90-utf8-1
*allow_bold: false

! ~~~~~~~~~~~~~~~~~~~~~
! color scheme
! ~~~~~~~~~~~~~~~~~~~~~
! special
*.foreground:   #ffffff
*.background:   #161616
*.cursorColor:  #B04F41

! black
*.color0:       #232323
*.color8:       #343434

! red
*.color1:       #994444
*.color9:       #bc6767

! green
*.color2:       #668866
*.color10:      #89a589

! yellow
*.color3:       #eebb66
*.color11:      #ffd693

! blue
*.color4:       #446c88
*.color12:      #698aa1

! magenta
*.color5:       #ee7777
*.color13:      #ffa6a6

! cyan
*.color6:       #468b8b
*.color14:      #6faaaa

! white
*.color7:       #dddddd
*.color15:      #efefef


!~~~~~~~~~~~~~~~~
! urxvt settings
!~~~~~~~~~~~~~~~~
*cursorBlink: 1
*letterSpace: -1
*geometry: 95x25
*perl-ext-common:  default,matcher
*urlLauncher:      chromium
*scrollBar: false
*internalBorder: 10

!~~~~~~~~~~~~~~~
! xft settings
!~~~~~~~~~~~~~~~
Xft.dpi:                    96
Xft.antialias:              true
Xft.rgba:                   subpixel
Xft.hinting:                true
Xft.hintstyle:              hintslight


! ~~~~~
! rofi
! ~~~~~

rofi.color-enabled: true

Property            Background      Foreground      Background Alt       Highlight BG      Highlight FG
rofi.color-window:    #252123,        #FFFFFF,        #252123,             #FFC715,          #252123
rofi.color-normal:    #252123,        #FFFFFF,        #252123,             #FFC715,          #252123
rofi.color-active:    #252123,        #FFFFFF,        #252123,             #FFC715,          #252123
rofi.color-urgent:    #252123,        #DDDDDD,        #252123,             #FFC715,          #252123
