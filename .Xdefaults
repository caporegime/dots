! No scrollbar
URxvt*scrollBar: False

! Transparency & Blur
urxvt*depth: 32
urxvt*background: rgba:3333/3333/3333/eeee

! Internal Padding
URxvt*internalBorder: 7

! Cursor Options
URxvt*cursorBlink: 1
URxvt*cursorColor:  #fff

! Modi
URxvt*modifier: alt

! Background color
URxvt*background: #272727

! Spacing
! URxvt*lineSpace: 0
! URxvt*letterSpace: 0

! Foreground color
URxvt*foreground: white

! Terminal dimensions
URxvt*geometry: 125x25

! Fix 256

URxvt*termName: rxvt

! Font
URxvt.font: -benis-lemon-medium-r-normal--10-110-75-75-m-50-iso10646-1
URxvt.boldFont: -benis-lemon-medium-r-normal--10-110-75-75-m-50-iso10646-1
! URxvt.font: -eti-limon-medium-r-normal--10-110-75-75-m-50-iso8859-1
! URxvt.boldFont: -eti-limon-medium-r-normal--10-110-75-75-m-50-iso8859-1
! URxvt.font: xft:Cure.se Medium:size=8


! Cursor

Xcursor.theme:   OSX-ElCap

! Clic-able links

URxvt.perl-ext-common:  default,matcher
URxvt.urlLauncher:      chromium

! Colors

! black
*color0:      #14161A
*color8:      #14161A
! red
*color1:      #8C5760
*color9:      #8C5760
! green
*color2:      #7B8C58
*color10:     #7B8C58
! yellow
*color3:      #B28C55
*color11:     #B28C55
! blue
*color4:      #58698C
*color12:     #58698C
! magenta
*color5:      #B23450
*color13:     #B23450
! cyan
*color6:      #737373
*color14:     #737373
! white
*color7:      #D7D7D7
*color15:     #D7D7D7

