! No scrollbar
URxvt*scrollBar: False

! Transparency
urxvt*depth: 32
urxvt*background: rgba:3333/3333/3333/e3e3

! Internal Padding
URxvt*internalBorder: 7

! Cursor Options
URxvt*cursorBlink: 1
URxvt*cursorColor:  #fff

! Modi
URxvt*modifier: alt

! Background color
URxvt*background: #272727

! Spacing
! URxvt*lineSpace: 2
! URxvt*letterSpace: -2

! Foreground color
URxvt*foreground: white

! Terminal dimensions
URxvt*geometry: 125x25

! Fix 256

URxvt*termName: rxvt

! Font

URxvt.font: -fuyuki-tangerine-medium-r-normal--10-110-75-75-m-50-iso8859-1
URxvt.boldFont: -fuyuki-tangerine-medium-r-normal--10-110-75-75-m-50-iso8859-1
! URxvt.font: xft:[FONT NAME]:size=12


! Cursor

Xcursor.theme:   OSX-ElCap

! Clic-able links

URxvt.perl-ext-common:  default,matcher
URxvt.urlLauncher:      chromium
URxvt.matcher.button:   1

! Colors

! special
*.foreground:   #EDEDED
!*.background:   #181818

! black
*.color0:       #525252
*.color8:       #747474

! red
*.color1:       #e55050
*.color9:       #c76363

! green
*.color2:       #c1c974
*.color10:      #e2e99d

! yellow
*.color3:       #faa65c
*.color11:      #ffb777

! blue
*.color4:       #58a3b8
*.color12:      #80b6bd

! magenta
*.color5:       #b77dca
*.color13:      #b29cb9

! cyan
*.color6:       #7cc7bd
*.color14:      #85b0aa

! white
*.color7:       #9785F2
*.color15:      #c5c8c6
