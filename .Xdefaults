# fonts
*font: -benis-lemon-medium-r-normal--10-110-75-75-m-50-iso10646-1
*boldFont: -benis-lemon-medium-r-normal--10-110-75-75-m-50-iso10646-1


# disable urxvt's ugly scrollbar
*scrollBar: false


# colorscheme
! special
*.foreground:   #ffffff
*.background:   #222222

! black
*.color0:       #232323
*.color8:       #343434

! red
*.color1:       #ed5554
*.color9:       #e78383

! green
*.color2:       #668866
*.color10:      #89a589

! yellow
*.color3:       #eebb66
*.color11:      #ffd693

! blue
*.color4:       #446c88
*.color12:      #698aa1

! magenta
*.color5:       #ee7777
*.color13:      #ffa6a6

! cyan
*.color6:       #468b8b
*.color14:      #6faaaa

! white
*.color7:       #dddddd
*.color15:      #efefef


# cursor
*.cursorColor:  #ed5554
*cursorBlink: 1


# size
*geometry: 90x22


# padding
*internalBorder: 10


# xft fonts settings
Xft.dpi:                    96
Xft.antialias:              true
Xft.rgba:                   subpixel
Xft.hinting:                true
Xft.hintstyle:              hintslight
Xft.lcdfilter:				lcddefault


# rofi settings
rofi.color-enabled: true

! Property            Background      Foreground      Background Alt       Highlight BG      Highlight FG
rofi.color-window:    #252525,        #FFFFFF,        #252525,             #ed5554,          #252525
rofi.color-normal:    #252525,        #FFFFFF,        #252525,             #ed5554,          #252525
rofi.color-active:    #252525,        #FFFFFF,        #252525,             #ed5554,          #252525 
rofi.color-urgent:    #252525,        #DDDDDD,        #252525,             #ed5554,          #252525
rofi.font: Source Sans Pro 12
