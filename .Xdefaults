! Urxvt Settings

! Pas de barre de défilement
URxvt*scrollBar: False

! Couleur d'arrière plan
URxvt*background: #303030

! Couleur d'avant plan blanc
URxvt*foreground: white

! Dimension du terminal
URxvt*geometry: 100x25

! Fix 256

URxvt*termName: rxvt

! Police

URxvt.font: xft:DejaVu Sans Mono:size=8
URxvt.boldFont: xft:DejaVu Sans Mono:size=8

! Cursor

Xcursor.theme:   Openzone Black Slim

! Lien cliquable
! Valeur possible pour l'option matcher button: 1=clic gauche, 2= clic milieu, 1=clic droit

URxvt.perl-ext-common:  default,matcher
URxvt.urlLauncher:      google-chrome-stable
URxvt.matcher.button:   1

! Colors

! special
*.foreground:   #c5c8c6
*.background:   #1d1f21
*.cursorColor:  #c5c8c6

! black
*.color0:       #282a2e
*.color8:       #373b41

! red
*.color1:       #e55050
*.color9:       #c76363

! green
*.color2:       #c1c974
*.color10:      #e2e99d

! yellow
*.color3:       #faa65c
*.color11:      #ffb777

! blue
*.color4:       #58a3b8
*.color12:      #80b6bd

! magenta
*.color5:       #b77dca
*.color13:      #b29cb9

! cyan
*.color6:       #7cc7bd
*.color14:      #85b0aa

! white
*.color7:       #707880
*.color15:      #c5c8c6


