! No scrollbar
URxvt*scrollBar: False

! Transparency & Blur
urxvt*depth: 32
urxvt*background: rgba:3333/3333/3333/e3e3

! Internal Padding
URxvt*internalBorder: 7

! Cursor Options
URxvt*cursorBlink: 1
URxvt*cursorColor:  #fff

! Modi
URxvt*modifier: alt

! Background color
URxvt*background: #272727

! Spacing
! URxvt*lineSpace: 2
! URxvt*letterSpace: -2

! Foreground color
URxvt*foreground: white

! Terminal dimensions
URxvt*geometry: 125x25

! Fix 256

URxvt*termName: rxvt

! Font

URxvt.font: -fuyuki-tangerine-medium-r-normal--10-110-75-75-m-50-iso8859-1
URxvt.boldFont: -fuyuki-tangerine-medium-r-normal--10-110-75-75-m-50-iso8859-1
! URxvt.font: xft:[FONT NAME]:size=12


! Cursor

Xcursor.theme:   OSX-ElCap

! Clic-able links

URxvt.perl-ext-common:  default,matcher
URxvt.urlLauncher:      chromium

! Colors

! special
*.foreground:   #f8f8f2

! black
URxvt*.color0:       #6272a4
URxvt*.color8:       #98a4c9

! red
URxvt*.color1:       #ff5555
URxvt*.color9:       #ff8787

! green
URxvt*.color2:       #2eba93
URxvt*.color10:      #4cde9f

! yellow
URxvt*.color3:       #f1fa8c
URxvt*.color11:      #f9ffb6

! blue
URxvt*.color4:       #81a2be
URxvt*.color12:      #b6cde1

! magenta
URxvt*.color5:       #bd93f9
URxvt*.color13:      #d8c0f9

! cyan
URxvt*.color6:       #8be9fd
URxvt*.color14:      #b8f2ff

! white
URxvt*.color7:       #f8f8f2
URxvt*.color15:      #ffffff


