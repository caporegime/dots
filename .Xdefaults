! No scrollbar
URxvt*scrollBar: False

! Background color
URxvt*background: #272727

!Spacing
URxvt*linespace: 10
URxvt*letterSpace: -2

! Foreground color
URxvt*foreground: white

! Terminal dimensions
URxvt*geometry: 100x25

! Fix 256

URxvt*termName: rxvt

! Font

URxvt.font: xft:Tangerine:size=8
URxvt.boldFont: xft:Tangerine:size=8

! Cursor

Xcursor.theme:   Openzone Black Slim

! Clic-able links
! Valeur possible pour l'option matcher button: 1=clic gauche, 2= clic milieu, 1=clic droit

URxvt.perl-ext-common:  default,matcher
URxvt.urlLauncher:      chromium
URxvt.matcher.button:   1

! Colors

! special
*.foreground:   #c5c8c6
*.background:   #181818
*.cursorColor:  #c5c8c6

! black
*.color0:       #282a2e
*.color8:       #373b41

! red
*.color1:       #e55050
*.color9:       #c76363

! green
*.color2:       #c1c974
*.color10:      #e2e99d

! yellow
*.color3:       #faa65c
*.color11:      #ffb777

! blue
*.color4:       #58a3b8
